             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="TestesEvent.View.Pagina1View"
             Title="Pagina 1">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="Center" >
            <Label x:Name="Teste" Text="Esta é a Pagina 1" />

            <Button x:Name="btnPagina2" Text="Acessar a página 2" Clicked="btnPagina2_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>